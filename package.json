{
  "name": "react-base-webpack4-babel-test",
  "version": "1.0.0",
  "author": "Pablo David Romero <pablo.romeropablo@gmail.com>",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js --hot",
    "start:reload": "webpack-dev-server --config=config/webpack.dev.js",
    "start:style": "cross-env MAP_STYLE=true webpack-dev-server --config=config/webpack.dev.js --hot",
    "build": "webpack --config=config/webpack.prod.js",
    "analyze": "cross-env ENABLE_ANALYZER=true npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
